import java.util.ArrayList;
import java.util.List;

public class TypeCastingProblem {

	public static void main(String[] args) {
		String[] s = new String[10];
		s[0] = "John";
		s[1] = "Rob";
		//wont work
		//s[2] = 123;
		
		List l = new ArrayList();
		l.add("John");
		l.add("Rob");
		l.add(123);
		
		String name = l.get(0);
	}

}
