import java.util.ArrayList;
import java.util.List;

public class TypeCastingProblem {

	public static void main(String[] args) {
		String[] s = new String[10];
		s[0] = "John";
		s[1] = "Rob";
		//wont work
		//s[2] = 123;
		
		//List l = new ArrayList();
		List<String> l = new ArrayList<>();
		l.add("John");
		l.add("Rob");
		//l.add(123); //error
		
		String name =  (String) l.get(0); //we have to typecast it, otherwise type mismatch object to string conversion not allowed
		String name1 = (String) l.get(1);
		String name2 = (String) l.get(2); //Class Cast exception: integer cannot be cast to java lang String
	}

}
