package maps;

import java.util.HashMap;
import java.util.Map;

public class WeakHashMapDemo {
	public static void main(String[] args) throws InterruptedException {
		Map<User, String> map = new WeakHashMap<>();
		User u = new User();
		map.put(u,"Gil");
		System.out.println(map);
		
		u = null;
		
		System.gc();
		Thread.sleep(3000);
		
		System.out.println(map);
	}
}
